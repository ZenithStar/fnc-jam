[sub_resource type="GDScript" id="GDScript_tyymd"]
script/source = "extends Node2D

@export var bullet: PackedScene

func fire():
	var direction: = global_position.angle_to_point(get_tree().get_first_node_in_group(\"Player\").global_position)
	match get_tree().get_first_node_in_group(\"GameServer\").difficulty:
		Global.Difficulty.EASY:
			var new_bullet := bullet.instantiate()
			new_bullet.rotation = direction
			new_bullet.position = global_position
			new_bullet.origin = global_position
			get_tree().get_first_node_in_group(\"BulletServer\").add_child.call_deferred(new_bullet)
		Global.Difficulty.NORMAL:
			for i in 3:
				var new_bullet := bullet.instantiate()
				new_bullet.rotation = direction
				new_bullet.position = global_position
				new_bullet.origin = global_position
				get_tree().get_first_node_in_group(\"BulletServer\").add_child.call_deferred(new_bullet)
				await get_tree().create_timer(0.2).timeout
		Global.Difficulty.HARD:
			for i in 5:
				var new_bullet := bullet.instantiate()
				new_bullet.rotation = direction
				new_bullet.position = global_position
				new_bullet.origin = global_position
				get_tree().get_first_node_in_group(\"BulletServer\").add_child.call_deferred(new_bullet)
				await get_tree().create_timer(0.2).timeout
		Global.Difficulty.LUNATIC:
			for i in 10:
				var new_bullet := bullet.instantiate()
				new_bullet.rotation = direction
				new_bullet.position = global_position
				new_bullet.origin = global_position
				get_tree().get_first_node_in_group(\"BulletServer\").add_child.call_deferred(new_bullet)
				await get_tree().create_timer(0.2).timeout
"

